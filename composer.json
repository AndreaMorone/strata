{
    "name": "francoisfaubert/strata",
    "type": "library",
    "description": "Strata is a MVC programming environment for Wordpress.",
    "keywords": [
        "wordpress",
        "mvc"
    ],
    "homepage": "http://github.com/francoisfaubert/strata",
    "license": "GPL",
    "authors": [
        {
            "name": "Francois Faubert",
            "email": "frank@francoisfaubert.com",
            "homepage": "http://francoisfaubert.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4",
        "vlucas/phpdotenv": "~2.0.1",
        "altorouter/altorouter": "~1.1.0",
        "symfony/console": "~2.6",
        "wp-cli/wp-cli": "~0.19.2",
        "gettext/gettext": "~3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.7.0",
        "phploc/phploc": "*",
        "theseer/phpdox": "~0.8.1.1",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Strata\\": "src"
        }
    },
    "support": {
        "issues": "https://github.com/francoisfaubert/strata/issues"
    },
    "prefer-stable" : true
